# popcorn-time
alias pop=/opt/Popcorn-Time/Popcorn-Time

# drawio
alias draw=$HOME/binaries/*draw.io*.AppImage

# obsidian
alias obsidian=$HOME/binaries/Obsidian*

# unity
alias u3=$HOME/binaries/UnityHub*

alias kdash="echo 'Go here !! http://localhost:8001/api/v1/namespaces/kubernetes-dashboard/services/https:kubernetes-dashboard:/proxy/' && kubectl -n kubernetes-dashboard describe secret $(kubectl -n kubernetes-dashboard get secret | grep admin-user | awk '{print $1}') && kubectl proxy"
